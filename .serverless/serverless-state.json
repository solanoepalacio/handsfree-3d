                    }
                  }
                ],
                "Enabled": true,
                "HttpVersion": "http2",
                "DefaultRootObject": "index.html",
                "Aliases": [
                  "myfirstthreejs.solano.be"
                ],
                "DefaultCacheBehavior": {
                  "AllowedMethods": [
                    "GET",
                    "HEAD"
                  ],
                  "Compress": true,
                  "TargetOriginId": "S3Origin",
                  "ForwardedValues": {
                    "QueryString": true,
                    "Cookies": {
                      "Forward": "none"
                    }
                  },
                  "ViewerProtocolPolicy": "redirect-to-https"
                },
                "PriceClass": "PriceClass_All",
                "ViewerCertificate": {
                  "AcmCertificateArn": "arn:aws:acm:us-east-1:742666682568:certificate/7906beb2-18c3-4629-a2ed-1718472d1091",
                  "SslSupportMethod": "sni-only"
                }
              }
            }
          },
          "PublicDNS": {
            "Type": "AWS::Route53::RecordSet",
            "Properties": {
              "HostedZoneName": "solano.be.",
              "Name": "myfirstthreejs.solano.be",
              "Region": "us-east-1",
              "Type": "A",
              "AliasTarget": {
                "HostedZoneId": "Z2FDTNDATAQYW2",
                "DNSName": {
                  "Fn::GetAtt": [
                    "UICloudfront",
                    "DomainName"
                  ]
                }
              },
              "SetIdentifier": "myfirstthreejs"
            }
          }
        },
        "Outputs": {
          "ServerlessDeploymentBucketName": {
            "Value": {
              "Ref": "ServerlessDeploymentBucket"
            }
